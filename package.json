{
  "name": "tfc-flow-transaction-service",
  "version": "1.0.0",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@onflow/types": "^1.0.5",
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "files": [
    "/dist/**/*.d.ts",
    "/dist/**/*.d.ts.map",
    "/dist/**/*.js",
    "/dist/**/*.js.map"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --build \"tsconfig.prod.json\" && npm run copy-cadence",
    "clean": "shx rm -rf ./dist",
    "ts-node-server": "ts-node src/index.ts",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "dev-server": "NODE_ENV=development nodemon",
    "prettier": "prettier --config \"./.prettierrc.yml\" --ignore-path \"./.prettierignore\" --write \"**/*.{js,json,md,ts,yml}\"",
    "prettier:check": "prettier --check --config \"./.prettierrc.yml\" --ignore-path \"./.prettierignore\" \"**/*.{js,json,md,ts,yml}\"",
    "typecheck": "tsc --noEmit",
    "copy-cadence": "shx cp -R ./src/cadence/transactions ./dist/cadence"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.2.2",
    "@google-cloud/pubsub": "^3.3.0",
    "@onflow/fcl": "^1.3.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "fcl-gcp-kms-authorizer": "^0.0.1-b",
    "flow-cadut": "^0.1.16-alpha.4",
    "fs": "^0.0.1-security",
    "ioredis": "^5.2.4",
    "path": "^0.12.7",
    "redis": "^4.5.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  }
}
